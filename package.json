{
  "name": "data-mocks",
  "version": "1.0.6",
  "main": "dist/mocks.js",
  "types": "dist/mocks.d.ts",
  "repository": "git@github.com:ovotech/data-mocks.git",
  "author": "Dave Cooper <dave.cooper@ovoenergy.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run build",
    "build": "npm run clean && tsc && babel dist/mocks.js --out-file dist/mocks.js",
    "test": "jest"
  },
  "jest": {
    "testRegex": "\\.(test|spec)\\.ts?$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "dependencies": {
    "fetch-mock": "^5.13.1"
  },
  "devDependencies": {
    "@types/fetch-mock": "^5.12.0",
    "@types/jest": "^21.1.2",
    "ts-jest": "^21.1.2",
    "jest": "^21.2.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "typescript": "^2.5.3"
  }
}
