{
  "name": "data-mocks",
  "version": "2.0.0",
  "main": "dist/mocks.js",
  "types": "dist/mocks.d.ts",
  "repository": "git@github.com:ovotech/data-mocks.git",
  "author": "Dave Cooper <dave.cooper@ovoenergy.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run build",
    "build": "npm run clean && tsc && babel dist/mocks.js --out-file dist/mocks.js",
    "test": "jest"
  },
  "jest": {
    "testRegex": "\\.(test|spec)\\.ts?$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "jest-environment-jsdom-global"
  },
  "dependencies": {
    "@types/query-string": "^6.1.0",
    "fetch-mock": "^5.13.1",
    "query-string": "^6.1.0"
  },
  "devDependencies": {
    "@types/fetch-mock": "^5.12.2",
    "@types/jest": "^23.1.4",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^23.3.0",
    "jest-environment-jsdom": "^23.3.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "ts-jest": "^23.0.0",
    "typescript": "^2.8.3"
  },
  "keywords": [
    "mocking",
    "mocks",
    "testing"
  ]
}
