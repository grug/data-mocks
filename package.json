{
  "name": "data-mocks",
  "version": "2.5.8",
  "main": "dist/mocks.js",
  "types": "dist/mocks.d.ts",
  "repository": "git@github.com:ovotech/data-mocks.git",
  "author": "Dave Cooper <dave.cooper@ovoenergy.com>",
  "license": "MIT",
  "homepage": "https://github.com/ovotech/data-mocks",
  "scripts": {
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run build",
    "build": "npm run clean && tsc && babel dist/mocks.js --out-file dist/mocks.js && uglifyjs dist/mocks.js -mco dist/mocks.js",
    "test": "jest",
    "publish:patch": "git checkout master && git pull && yarn version --patch && yarn publish --non-interactive && git push",
    "publish:minor": "git checkout master && git pull && yarn version --minor && yarn publish --non-interactive && git push",
    "publish:major": "git checkout master && git pull && yarn version --major && yarn publish --non-interactive && git push"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testRegex": "\\.(test|spec)\\.ts?$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "dependencies": {
    "fetch-mock": "^7.3.1",
    "node-fetch": "^2.2.0",
    "query-string": "^5.1.1",
    "xhr-mock": "^2.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@types/fetch-mock": "^7.2.3",
    "@types/jest": "^24.0.9",
    "@types/query-string": "^5.1.0",
    "axios": "^0.18.1",
    "husky": "^1.1.1",
    "jest": "^24.3.1",
    "prettier": "1.14.3",
    "pretty-quick": "^1.7.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333",
    "uglify-js": "^3.4.9"
  },
  "keywords": [
    "mocking",
    "mocks",
    "testing"
  ]
}
